{
  "name": "networkego",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "uv:install": "bash -lc 'command -v uv >/dev/null 2>&1 || curl -LsSf https://astral.sh/uv/install.sh | sh'",
    "py:setup": "bash -lc 'export PATH=\"$HOME/.local/bin:$HOME/.cargo/bin:$PATH\"; uv venv --seed .venv && . .venv/bin/activate && uv pip install --python .venv/bin/python --index-url https://download.pytorch.org/whl/cpu torch torchvision tqdm'",
    "py:run": "bash -lc '. .venv/bin/activate && python runner/run.py'",
    "py:setup-run": "npm run uv:install && npm run py:setup && npm run py:run",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "lucide-react": "^0.471.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "reactflow": "^11.11.4",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prismjs": "^1.30.0",
    "vite": "5.4.2"
  }
}
